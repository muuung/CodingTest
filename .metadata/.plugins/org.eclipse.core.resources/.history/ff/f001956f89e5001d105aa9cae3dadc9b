package step2;
import java.math.*;

/* 시소 짝꿍 */
public class P152996 {
    public long solution(int[] weights) {
        long answer = 0;
        int num = 0;
        
        for(int i = 0; i < weights.length-1; i++) {
        	for(int j = i+1; j < weights.length; j++) {
        		
        		BigInteger gcd = BigInteger.valueOf(weights[i]).gcd(BigInteger.valueOf(weights[j]));
        		num = gcd.intValue();
        		
        		if(weights[j]/num == 2 || weights[j]/num == 3 ||
        	       weights[j]/num == 4 || weights[j] == num)
        			answer += 1;
        	}
        }
        
        return answer;
    }
}