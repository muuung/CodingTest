package step2;

/* 시소 짝꿍 */
public class P152996 {
    public long solution(int[] weights) {
        long answer = 0;
        String a = "";
        
        for(int i = 0; i < weights.length-1; i++) {
        	for(int j = i+1; j < weights.length; j++) {
        		
        		a = Double.toString((double)weights[i]/weights[j]);
        		
        		switch(a) {
        		case 1.0:
          		case 2.0:
        		case 1/2:
        		case 2/3:
        		case 3/2:
        		case 3/4:
        		case 4/3:
        			answer++;
        			break;
        		}
        		
        		
        	}
        }
        
        return answer;
    }
}